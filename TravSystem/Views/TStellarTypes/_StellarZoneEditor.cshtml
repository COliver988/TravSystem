@model TravSystem.Models.TStellarZones

<div class="stellarzone-editor">
    <div class="form-group">
        <label asp-for="Zone" class="control-label"></label>
        <input asp-for="Zone" class="form-control" />
        <span asp-validation-for="Zone" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Orbit" class="control-label"></label>
        <input asp-for="Orbit" class="form-control" />
        <span asp-validation-for="Orbit" class="text-danger"></span>
    </div>
</div>
